{
  "name": "ddr1nk/phpstan-html-output-formatter",
  "description": "PHPStan HTML error formatter",
  "type": "phpstan-extension",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "phpstan/phpstan": "^1.10 || ^2.0",
    "phpstan/extension-installer": "^1.3",
    "smarty/smarty": "^4.5"
  },
  "autoload": {
    "psr-4": {
      "DDr1nk\\PhpstanHtmlOutputFormatter\\": "src/"
    }
  },
  "extra": {
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
